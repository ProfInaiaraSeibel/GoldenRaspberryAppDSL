<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <TextView
        android:id="@+id/yearsMultipleWinners"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Anos com mais de um vencedor" />

    <TextView
        android:id="@+id/topStudios"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Estúdios com mais vitórias" />

    <TextView
        android:id="@+id/producersInterval"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Intervalo entre prêmios (Produtores)" />
</LinearLayout>
